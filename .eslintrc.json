{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "max-classes-per-file": "off",
      "no-param-reassign": "off",
      "no-unused-vars": "warn",
      "no-unused-expressions": "warn",
      "class-methods-use-this": "warn",
      "no-unneeded-ternary": "warn",
      "consistent-return": "off",
      "no-use-before-define": "off",
      "no-undef": "warn",
      "space-in-parens": "off"
    }
}
